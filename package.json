{
  "name": "liteapi-map-sdk",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": " vite build && tsc -p tsconfig.build.json",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "4.0.5",
    "jsdom": "^27.0.1",
    "typescript": "~5.9.3",
