    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.5"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.1.14"
  },
  "dependencies": {
    "mapbox-gl": "^3.16.0"
  },
  "main": "dist/liteapi-map-sdk.umd.js",
  "module": "dist/liteapi-map-sdk.es.js",
  "types": "dist/types/index.d.ts",
